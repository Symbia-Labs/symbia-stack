{
  "name": "@symbia/messaging-client",
  "version": "1.0.0",
  "description": "Messaging service client - REST API, WebSocket, and pub/sub for Symbia services",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.js",
      "require": "./dist/client.js",
      "default": "./dist/client.js"
    },
    "./socket": {
      "types": "./dist/socket.d.ts",
      "import": "./dist/socket.js",
      "require": "./dist/socket.js",
      "default": "./dist/socket.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js",
      "require": "./dist/types.js",
      "default": "./dist/types.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist"
  },
  "keywords": ["symbia", "messaging", "conversations", "websocket", "realtime"],
  "author": "Symbia",
  "license": "MIT",
  "dependencies": {
    "socket.io-client": "^4.8.3"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "socket.io-client": "^4.0.0"
  },
  "peerDependenciesMeta": {
    "socket.io-client": {
      "optional": true
    }
  }
}
